<h2>Welcome you</h2>

<% if current_user %>
    <p>
        You are currently logged in as <%= current_user.email %>
    </p>
<%= link_to "Log Out", log_out_path, method: :delete %>
<% else %>
    <%= link_to "Log In", log_in_path %>
    <%= link_to "Sign Up", new_user_path %>
<% end %>

<ul>
    <% @users.each do |user| %>
        <li>
            <%= user.email %>
        </li>
    <% end %>
</ul>